{
  "log": {
    "disabled": true,
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dn_cloudflare",
        "address": "1.1.1.1",
        "detour": "ot_socks"
      }
    ],
    "rules": [
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dn_cloudflare"
      }
    ],
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "in_tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "strict_route": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "ot_direct"
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "in_tun",
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "outbound": "ot_socks"
      }
    ],
    "auto_detect_interface": true
  }
}